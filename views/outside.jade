div.container

	div#MapDiv

	div.hero-unit
		ul
			for x in data.results
				li= x.name


script
	$(document).ready(function() {
		var map = new EMS.Services.Map("MapDiv");
		navigator.geolocation.getCurrentPosition(function (position) {

			var geocoder = new EMS.Services.Geocoder();
			geocoder.reverseGeocode(position.coords.longitude, position.coords.latitude, function(addr) {
				alert(JSON.stringify(addr));
			});
		});
	});
